<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.rodrigo.aprendigame">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_app_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.QuestionarioActivity"></activity>
        <activity android:name=".Activity.PresencaRealizadaActivity" />
        <activity android:name=".Activity.CourseUnitActivity" />
        <activity android:name=".Activity.CadastroActivity" />
        <activity android:name=".Activity.MainActivity"/>
        <activity android:name=".Activity.EditarPerfilActivity" />
        <activity android:name=".Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>